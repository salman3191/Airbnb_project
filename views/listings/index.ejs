<% layout("/layouts/boilerplate") %>

<body>
  <h1>All listings</h1>

  <form method="get" action="/listings/new">
    <button>Create New</button>
  </form>
  <ul>
    <%for(listings of allListings){%>

    <li><a href="/listings/<%= listings._id %>"><%= listings.title %></a></li>

    <%}%>
  </ul>
</body>
